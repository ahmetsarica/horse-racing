            "date": 1732226823300,
            "name": "Commit-0",
            "content": "<template>\r\n  <!-- This is the main template for the HorseList component -->\r\n  <div class=\"flex flex-col h-full overflow-auto\">\r\n    <!-- If there are horses in the store, display them -->\r\n    <template v-if=\"horses && horses.length > 0\">\r\n      <div class=\"text-center font-bold mb-4 text-xl\">HORSE LIST (1-20)</div>\r\n      <table class=\"min-w-full border border-gray-200 bg-white\">\r\n        <thead class=\"bg-gray-200 text-gray-600 uppercase text-sm font-bold\">\r\n          <tr>\r\n            <th class=\"py-3 px-6 border-r border-gray-300 w-3/6\">Name</th>\r\n            <th class=\"py-3 px-6 border-r border-gray-300 w-2/6\">Condition</th>\r\n            <th class=\"py-3 px-6 w-1/6\">Color</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <!-- For each horse, display its details -->\r\n          <tr\r\n            v-for=\"horse in horses\"\r\n            :key=\"horse.id\"\r\n            class=\"border-t border-gray-200 hover:bg-gray-100\"\r\n          >\r\n            <td class=\"py-3 px-6 border-r border-gray-300 w-3/6\">\r\n              {{ horse.name }}\r\n            </td>\r\n            <td class=\"py-3 px-6 border-r border-gray-300 w-2/6\">\r\n              {{ horse.condition }}\r\n            </td>\r\n            <td\r\n              class=\"py-3 px-6 w-1/6 font-bold\"\r\n              :style=\"{ color: horse.color }\"\r\n            >\r\n              {{ horse.color }}\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </template>\r\n    <!-- If there are no horses in the store, display a message -->\r\n    <template v-else>\r\n      <BlankPage\r\n        title=\"No Horses Found\"\r\n        description=\"Please click the Generate Program button to create horses\"\r\n      />\r\n    </template>\r\n  </div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport { defineComponent, computed } from 'vue';\r\nimport { useStore } from 'vuex';\r\nimport BlankPage from '@/components/blankPage/BlankPage.vue';\r\n\r\nexport default defineComponent({\r\n  name: 'HorseList',\r\n  components: {\r\n    BlankPage,\r\n  },\r\n  setup() {\r\n    const store = useStore();\r\n    const horses = computed(() => store.getters['getAllHorses']);\r\n\r\n    return {\r\n      horses,\r\n    };\r\n  },\r\n});\r\n</script>\r\n"
        }
